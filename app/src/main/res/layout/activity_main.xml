<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/textView001" />

    <WebView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/webView1"
        android:layout_below="@+id/textView001"/>

    <GridView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gridView001"
        android:layout_below="@+id/webView1" />



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get button"
        android:id="@+id/Button001"
        android:layout_marginStart="400dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Post button"
        android:id="@+id/Button002"
        android:layout_below="@+id/Button001"
        android:layout_marginStart="400dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Delete button"
        android:id="@+id/Button003"
        android:layout_below="@+id/Button002"
        android:layout_marginStart="400dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Flickr button"
        android:id="@+id/Button004"
        android:layout_below="@+id/Button003"
        android:layout_marginStart="400dp" />


    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText1"
        android:layout_marginStart="200dp"
        android:inputType="number"
        android:text="10" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText2"
        android:layout_marginStart="200dp"
        android:text="DefaultName"
        android:layout_below="@+id/editText1" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView001"
        android:layout_marginStart="200dp"
        android:layout_below="@+id/editText2" />




</RelativeLayout>
